;; -*- mode: fennel; -*- vi:ft=fennel

(local {: run-tests}
  (require :io.gitlab.andreyorst.fennel-test))

(fn select-tests [default-tests]
  (case [(select 1 ((or _G.unpack table.unpack) arg))]
    (where tests (next tests)) tests
    _ default-tests))

(local tests
  [:figuring-it-out])

(-> tests
    select-tests
    run-tests)

